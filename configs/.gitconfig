[user]
	name = Kent Martin Ã…svang
	email = kent@asvang.no
[ui "color"]
	ui = always
[color]
	ui = true
[init]
	defaultBranch = main
[alias]
	c = commit
	s = status
	st = status
	lg = log --oneline
	last = log -1 HEAD
	ch = checkout
	diffn = diff --name-only
[pull]
	ff = only
[push]
	default = simple
[help]
	autocorrect = 1
[url "git.launchpad.net"]
	insteadOf = lp:
[url "git@g.csail.mit.edu:"]
	insteadOf = g:
[url "git@github.com:"]
	insteadOf = gh:
[github]
	user = kentaasvang
[diff]
	algorithm = patience
	compactionHeuristic = true
[diff "odf"]
	textconv = odt2txt
